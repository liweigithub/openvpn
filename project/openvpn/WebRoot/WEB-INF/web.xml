<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <filter>
    <filter-name>UserFilter</filter-name>
    <filter-class>com.openvpn.filter.UserFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>UserFilter</filter-name>
    <url-pattern>/admin/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.openvpn.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.openvpn.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AdminServlet</servlet-name>
    <servlet-class>com.openvpn.servlet.AdminServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>page</servlet-name>
    <servlet-class>com.openvpn.servlet.page</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserLog</servlet-name>
    <servlet-class>com.openvpn.servlet.UserLog</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserListServlet</servlet-name>
    <servlet-class>com.openvpn.servlet.UserListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddUserServlet</servlet-name>
    <servlet-class>com.openvpn.servlet.AddUserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserInfoServlet</servlet-name>
    <servlet-class>com.openvpn.servlet.UserInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/user.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/user_register.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/admin/admin.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>page</servlet-name>
    <url-pattern>/admin/page.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserLog</servlet-name>
    <url-pattern>/admin/userlog.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserListServlet</servlet-name>
    <url-pattern>/admin/user_list.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddUserServlet</servlet-name>
    <url-pattern>/admin/add_user.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserInfoServlet</servlet-name>
    <url-pattern>/admin/user_info.html</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>/</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>UserTestServlet</display-name>
    <servlet-name>UserTestServlet</servlet-name>
    <servlet-class>com.openvpn.servlet.UserTestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserTestServlet</servlet-name>
    <url-pattern>/user_register.do</url-pattern>
  </servlet-mapping>
</web-app>